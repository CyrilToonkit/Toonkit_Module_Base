                </property>  
                <property name='tk_invertUp'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
                </property> 
                <property name='tk_guideParent'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
                </property> 
            </group>
            <group name='InitValues'>
                <property name='initValues'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
                </property>
                <property name='guideHelpers'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
                </property>

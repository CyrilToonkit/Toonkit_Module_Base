<?xml version='1.0' encoding='UTF-8'?>
<templates>

    <template name='AEtransform'>
                <attribute name='size' type='maya.float'>
                        <label>Size</label>
                </attribute>
                <attribute name='offsetDisplayX' type='maya.float'>
                        <label>X Offset</label>
                </attribute>
                <attribute name='offsetDisplayY' type='maya.float'>
                        <label>Y Offset</label>
                </attribute>
                <attribute name='offsetDisplayZ' type='maya.float'>
                        <label>Z Offset</label>
                </attribute>
                <attribute name='scaleDisplayX' type='maya.float'>
                        <label>X Scale</label>
                </attribute>
                <attribute name='scaleDisplayY' type='maya.float'>
                        <label>Y Scale</label>
                </attribute>
                <attribute name='scaleDisplayZ' type='maya.float'>
                        <label>Z Scale</label>
                </attribute>
                <attribute name='tk_type' type='maya.enum'>
                        <label>Oscar Type</label>
                </attribute>
                <attribute name='tk_guideRule' type='maya.enum'>
                        <label>Guide rule</label>
                </attribute>
                <attribute name='tk_placeHolder' type='maya.string'>

                </attribute>
                <attribute name='tk_target' type='maya.string'>
                        <label>Target</label>
                </attribute>
                <attribute name='tk_invert' type='maya.bool'>
                        <label>Invert</label>
                </attribute>
                <attribute name='tk_invertUp' type='maya.bool'>
                        <label>Invert Up</label>
                </attribute>
                <attribute name='tk_guideParent' type='maya.string'>
                        <label>Guide Parent</label>
                </attribute>
                <attribute name='initValues' type='maya.string'>
                        <label>Init Values</label>
                </attribute>
                <attribute name='guideHelpers' type='maya.string'>
                        <label>Guide Helpers</label>
                </attribute>
    </template>

    <view name="DisplaysView" template="AEtransform">
            <group name='Displays'>
                <property name='size'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
                </property>
                <property name='offsetDisplayX'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
                </property>
                <property name='offsetDisplayY'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
                </property>
                <property name='offsetDisplayZ'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
                </property>
                <property name='scaleDisplayX'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
                </property>
                <property name='scaleDisplayY'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
                </property>
                <property name='scaleDisplayZ'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
                </property>
            </group>
            <group name='ElementInfo'>
                 <property name='tk_type'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
                </property>
                <property name='tk_guideRule'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
                </property>
                <property name='tk_placeHolder'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
                </property>
                <property name='tk_upVReference'>
                        <description language="cb">py.tkMayaCore.AEaddDisplaysDoubleMenu</description>
